@model SixthWeb.Models.Material

<h2>Статья: @Model.Name</h2>
<div>

    <dl class="dl-horizontal" style="float:left">
        @Html.Raw(Model.Text)
    </dl>

    <a class="btn btn-primary" asp-action="Edit" asp-controller="Material" asp-route-id="@Model.Id">Изменить</a> |
    <a class="btn btn-danger" asp-action="Delete" asp-controller="Material" asp-route-id="@Model.Id">Удалить</a>
</div>